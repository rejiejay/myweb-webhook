    "react-markdown": "^3.3.0",
    "vconsole": "^3.1.0",
    "egg": "^2.8.1",
    "egg-cors": "^2.0.0",
    "egg-mysql": "^3.0.0",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "babel-plugin-dva-hmr": "^0.3.2",
    "eslint": "^4.14.0",
